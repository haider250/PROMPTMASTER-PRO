# Environment Variables Template for PROMPTMASTER PRO
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration (Free tier - 50K users, 500MB)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Stripe Configuration (For payment processing)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Google Analytics (Optional - for tracking)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Email Service Configuration (Optional)
VITE_EMAIL_SERVICE_URL=https://your-email-service.com/api/subscribe

# Development Mode (set to 'false' for production)
VITE_DEV_MODE=true